.fav-hint {
    position: fixed;
    top: 18rpx;
    right: 25rpx;
    padding: 0.5em 1em;
    width: 11em;
    border-radius: 50px;
    background-image: linear-gradient(90deg,#ffef2a,#ffd100);
    font-size: 26rpx;
    line-height: 1.2em;
    text-align: left;
    z-index: 2001;
    box-shadow: 0 0 1px transparent,0 2px 4px transparent;
    animation: dotFlash 1s ease-in-out 10s forwards;
}

.fav-hint.en {
    width: 14em;
}

.fav-hint::before {
    top: -14rpx;
    border-left: 15rpx solid transparent;
    border-right: 15rpx solid transparent;
    border-bottom: 15rpx solid #feda36;
    z-index: 2001;
}

.fav-hint::after,.fav-hint::before {
    content: "";
    position: absolute;
    right: calc(71.33px - 25rpx);
    width: 0rpx;
    height: 0rpx;
    transform: translateX(50%);
}

.fav-hint::after {
    top: -16rpx;
    border-left: 16rpx solid transparent;
    border-right: 16rpx solid transparent;
    border-bottom: 16rpx solid #feda36;
}

.navbar-header {
