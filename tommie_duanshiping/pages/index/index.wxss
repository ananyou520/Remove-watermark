.close-fav-hint {
    position: absolute;
    right: 0.8em;
    top: 55%;
    width: 1.0em;
    height: 1.0em;
    border-radius: 50%;
    background-color: #FFEF2A;
    transform: translateY(-50%);
}
.close-fav-hint::after,.close-fav-hint::before {
    content: "";
    position: absolute;
    left: 50%;
    top: 50%;
    width: .7em;
    height: 4rpx;
    border-radius: 2rpx;
    background-color: #4d4d4d;
}
.close-fav-hint::before {
    transform: translate(-50%, -50%) rotate(45deg);
}
.close-fav-hint::after {
    transform: translate(-50%, -50%) rotate(-45deg);
}
