page {
    background: #f2f2f2;
}

.video-box {
    display: block;
    width: 100%;
    height: 450rpx;
    margin: 0rpx 0rpx 44rpx 0;
}

.box {
    width:100%;
    height:450rpx;
    position:relative;
    overflow:hidden;
    display:flex;
    align-items:center;
    justify-content:center;
    background-color:#000;
    margin: 0rpx 0 44rpx 0;
}

.box,.images {
    display: flex;
    justify-content: center;
}

.images {
    height: 100%;
    position: absolute;
    width: 0 auto;
}

.title {
    display: block;
    width: 100%;
    height: 450rpx;
    margin: 0 0 40rpx;
}

.txtbox {
    width: 94%;
    height: 430rpx;
    display: flex;
    justify-content: center;
    background-color: #fff;
    border-radius: 20rpx;
    margin: 10rpx 0 0rpx 24rpx;
}

.parsing {
    margin-top: 30px;
}

.go-back,.parsing {
    width: 80%;
    background-color: #000;
    color: #fff;
    border-radius: 10rpx;
    background: #fc7e43;
}

.go-back {
    margin-top: 10px;
}

.bt1 {
    align-items: center;
    background: #26941c;
    display: flex;
    flex-direction: row;
    float: left;
    justify-content: center;
    margin-left: 10rpx;
}

.bt1,.bt2 {
    border: none;
    border-radius: 10rpx;
    color: #fff;
    font-size: 15px;
    height: 35px;
    margin-top: 10px;
    width: 45%;
}

.bt2 {
    float: right;
    margin-right: 20rpx;
}

.bt3 {
    background: #4a99e2;
    float: left;
    margin-left: 19.5rpx;
    margin-top: 10px;
}

.bt3,.bt4 {
    align-items: center;
    border: none;
    border-radius: 10rpx;
    color: #fff;
    display: flex;
    flex-direction: row;
    font-size: 15px;
    height: 35px;
    justify-content: center;
    width: 45%;
}

.bt4 {
    background: #6c66fe;
    float: right;
    margin: 20rpx 20rpx 0;
}

.bt5,.bt6,.bt7 {
    border-radius: 10rpx;
    float: right;
    font-size: 15px;
    height: 35px;
    margin: 30rpx 17rpx 0;
    width: 95.5%;
}

.bt7 {
    color: #fff;
}

.ad {
    display: block;
    width: 100%;
    margin: 0rpx 0;
}

.swiper-tab {
    z-index: 1;
    width: 100%;
    border-bottom: 2rpx solid#00000000;
    text-align: center;
    line-height: 80rpx;
}

.swiper-tab-list {
    font-size: 30rpx;
    display: inline-block;
    background-color: #fff;
    width: 33.33%;
    letter-spacing: 5rpx;
    color: #4e4e4e;
}

.on {
    border-bottom: 6rpx solid#ff3300;
    color: #f30;
}


.swiper-box {
    display: block;
    height: 830rpx;
    width: 100%;
    overflow: hidden;
}

.swiper-box1 view {
    text-align: center;
}

.text {
    margin: 5px 0 0 10px;
}

.text,.text2 {
    display: flex;
    bottom: 5%;
    font-size: 14px;
    letter-spacing: 3rpx;
}

.text2 {
    margin: -22px 0 0 121px;
}

.list {
    padding: 17rpx;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
}

.list .list-half {
    width: 350rpx;
    box-sizing: border-box;
}

.list-half .card {
    background-color: #fff;
    border-radius: 10rpx;
    overflow: hidden;
    margin-bottom: 20rpx;
    padding-bottom: 6rpx;
}

.slide-image {
    width: 100%;
    height: 80%;
}

.percent {
    width: 96%;
    margin: -38rpx 10rpx -10rpx 20rpx;
}

.wx-progress-inner-bar {
    border-radius: 20px!important;
    background: #fc7e43!important;
}

.list1 {
    display: flex;
}

.title {
    bottom: 5%;
    font-size: 16px;
    margin: 20px 20px 0;
    text-align: left;
}

.title,.title1 {
    display: flex;
    letter-spacing: 3rpx;
}

.title1 {
    margin: -10rpx 2% 0 0;
    padding: 6rpx 0 0 5rpx;
    color: #000;
}

.title1,.title2 {
    font-size: 14px;
    align-items: center;
    vertical-align: center;
    justify-content: center;
    flex-direction: row;
}

.title2 {
    margin: -10rpx 0 0 2%;
    padding: 5rpx 6rpx 0 0;
    color: rgba(0,0,0,.904);
    display: flex;
    letter-spacing: 3rpx;
}

.toast {
    font-size: 27rpx;
    z-index: 100;
    border-radius: 20rpx;
    background: rgba(0,0,0,.849);
    top: 40%;
    margin-top: -100rpx;
}

.prograss,.toast {
    position: fixed;
    width: 260rpx;
    height: 220rpx;
    color: #fff;
    left: 50%;
    margin-left: -130rpx;
    line-height: 260rpx;
    text-align: center;
}

.prograss {
    font-size: 28rpx;
    top: 26%;
    margin-top: 62rpx;
}

.pro {
    width: 220rpx;
    margin: -70rpx auto 0;
}

.down {
    color: rgba(0,0,0,.904);
    font-weight: 300;
    font-size: 33rpx;
    margin: 40rpx 17rpx 15rpx;
}

.image {
    width: 100%;
    height: 500rpx;
    margin: 10rpx 20rpx 20rpx 0rpx;
    float: left;
}